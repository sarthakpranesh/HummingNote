{
  "expo": {
    "experiments": {
      "turboModules": true
    },
    "name": "HummingNote",
    "description": "Note taking app with modern UI",
    "slug": "HummingNote",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "primaryColor": "#000000",
    "icon": "./assets/images/icon.png",
    "scheme": "humming-note",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.HummingNote"
    },
    "android": {
      "package": "com.HummingNote"
    }
  }
}
