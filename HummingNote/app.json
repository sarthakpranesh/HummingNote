{
  "expo": {
    "experiments": {
      "turboModules": true
    },
    "name": "HummingNote",
    "description": "Note taking app with modern UI",
    "slug": "humming-note",
    "version": "0.1.1",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#1b262c",
    "primaryColor": "#1b262c",
    "icon": "./assets/images/icon.png",
    "scheme": "hummingnote",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1b262c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hummingnote"
    },
    "android": {
      "package": "com.hummingnote",
      "config": {
        "googleSignIn": {
          "apiKey": "",
          "certificateHash": ""
        }
      }
    }
  }
}
